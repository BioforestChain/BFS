    <application
        android:name="info.bagen.dwebbrowser.App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.plaoc"
        android:usesCleartextTraffic="true">
        <profileable
            android:shell="true"
            tools:targetApi="29" />

        <activity
            android:name="info.bagen.dwebbrowser.SplashActivity"
            android:excludeFromRecents="true"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/Theme.plaoc">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="info.bagen.dwebbrowser.microService.browser.BrowserActivity"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/Theme.plaoc"
            android:screenOrientation="portrait" />
        <!--            android:launchMode="singleInstance"-->
        <activity
            android:name="info.bagen.dwebbrowser.microService.sys.mwebview.MultiWebViewPlaceholder1Activity"
            android:exported="true"
            android:windowSoftInputMode="adjustPan" />
        <activity
            android:name="info.bagen.dwebbrowser.microService.sys.mwebview.MultiWebViewPlaceholder2Activity"
            android:exported="true"
            android:windowSoftInputMode="adjustPan" />
        <activity
            android:name="info.bagen.dwebbrowser.microService.sys.mwebview.MultiWebViewPlaceholder3Activity"
            android:exported="true"
            android:windowSoftInputMode="adjustPan" />
        <activity
            android:name="info.bagen.dwebbrowser.microService.sys.mwebview.MultiWebViewPlaceholder4Activity"
            android:exported="true"
            android:windowSoftInputMode="adjustPan" />
        <activity
            android:name="info.bagen.dwebbrowser.microService.sys.mwebview.MultiWebViewPlaceholder5Activity"
            android:exported="true"
            android:windowSoftInputMode="adjustPan" />

        <meta-data
            android:name="com.google.firebase.ml.vision.DEPENDENCIES"
            android:value="barcode" />

        <activity
            android:name="info.bagen.dwebbrowser.microService.sys.plugin.share.ShareActivity"
            android:exported="true"
            android:theme="@style/Theme.AppCompat.Transparent">
            <intent-filter>
                <action android:name="info.bagen.dwebbrowser.share" />
            </intent-filter>
        </activity>
        <activity
            android:name="info.bagen.dwebbrowser.microService.sys.plugin.biometrics.BiometricsActivity"
            android:exported="true"
            android:theme="@style/Theme.AppCompat.Transparent"></activity>

        <activity
            android:name="info.bagen.dwebbrowser.microService.sys.jmm.ui.JmmManagerActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="info.bagen.dwebbrowser.openjmm" />
            </intent-filter>
        </activity>

        <service android:name="info.bagen.dwebbrowser.service.DwebBrowserService" />

        <provider
            android:name="androidx.core.content.FileProvider"
            android:authorities="info.bagen.dwebbrowser.file.opener.provider"
            android:exported="false"
            android:grantUriPermissions="true">
            <meta-data
                android:name="android.support.FILE_PROVIDER_PATHS"
                android:resource="@xml/file_opener_paths" />
        </provider>
